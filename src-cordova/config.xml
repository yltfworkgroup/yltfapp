<?xml version='1.0' encoding='utf-8'?>
<widget id="com.yltf.app" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>YltfProject</name>
    <description>quasar + cordova + scss</description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
        <icon src="res/android/icons/ic_launcher.png" />
        <icon density="ldpi" src="res/android/icons/mipmap-ldpi/ic_launcher.png" />
        <icon density="mdpi" src="res/android/icons/mipmap-mdpi/ic_launcher.png" />
        <icon density="hdpi" src="res/android/icons/mipmap-hdpi/ic_launcher.png" />
        <icon density="xhdpi" src="res/android/icons/mipmap-xhdpi/ic_launcher.png" />
        <icon density="xxhdpi" src="res/android/icons/mipmap-xxhdpi/ic_launcher.png" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <allow-navigation href="about:*" />
</widget>
